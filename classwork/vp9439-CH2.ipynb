    "import numpy as np\n",
    "\n",
    "def convertToBinary(x):\n",
    "    output = \"\"\n",
    "    while(x > 0):\n",
    "        output += str(x % 2)\n",
    "        x = x // 2\n",
    "    output = int(output[::-1])\n",
    "    print(output)"
   ]
